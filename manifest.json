{
  "name": "Lite",
  "description": "Lite new tab with tasks and clock",
  "version": "1.0",
  "incognito": "split",
  "permissions": ["activeTab", "storage"],
  "chrome_url_overrides": { "newtab": "newtab.html" },
  "content_scripts": [

    {

      "js": [
        "js/jq.js"
      ],
      "matches": ["*://*/*"],
      "run_at": "document_start"
    }

  ],
  "manifest_version": 2
}
